{
  "name": "spawndeck-terminal",
  "version": "0.1.0",
  "description": "Electron-based terminal for Spawndeck",
  "main": "src/main/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "cross-env ELECTRON_DISABLE_SANDBOX=1 ELECTRON_NO_ATTACH_CONSOLE=1 NODE_OPTIONS= npx electron . --no-sandbox",
    "rebuild": "electron-rebuild",
    "postinstall": "electron-rebuild"
  },
  "keywords": [
    "electron",
    "terminal"
  ],
  "author": "Spawndeck",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^22.3.27",
    "electron-rebuild": "^3.2.9",
    "@babel/core": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/standalone": "^7.22.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.0",
    "@dnd-kit/sortable": "^7.0.0",
    "@dnd-kit/utilities": "^3.2.0",
    "node-pty": "^0.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-webgl": "^0.16.0",
    "zustand": "^4.4.0"
  }
}
