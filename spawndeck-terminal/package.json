{
  "name": "spawndeck-terminal",
  "version": "0.1.0",
  "description": "Electron-based terminal for Spawndeck",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "electron .",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "webpack --config webpack.config.js --target electron-main",
    "build:preload": "webpack --config webpack.config.js --target electron-preload",
    "build:renderer": "webpack --config webpack.config.js --target web"
  },
  "keywords": ["electron", "terminal"],
  "author": "Spawndeck",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  }
}